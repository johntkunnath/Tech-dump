website = input( " website :" )
webpage_banner = input( "Webpage Banner :" )
sample_story = input ( "Sample Story :" )
sample_story_ends = input ( "Sample Story ends :" )
print ( "<html> \n <head> \n\t <title>")
print ( " \t\t " , website , "\n\t <title> \n <head>")
print ()
print ( "\n <body>" )
print ( "\t <h1> ", webpage_banner , "</h1>" )
print ()
print ( "\t <p>", sample_story , "\n\t </p> " )
print ()
print ( "\t <p>", sample_story_ends , "\n\t </p> " )
print ( "\n <body> " )
print ( "</html>" )
